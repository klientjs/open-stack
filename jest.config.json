{
  "verbose": true,
  "collectCoverage": true,
  "collectCoverageFrom": ["**/src/**", "!**/src/__tests__/**"],
  "coverageDirectory": "coverage",
  "coverageProvider": "babel",
  "coverageReporters": ["lcov", "json-summary"],
  "transform": {
    "\\.(ts)$": "ts-jest"
  },
  "testMatch": ["**/*.test.ts"]
}
