{
  "verbose": true,
  "collectCoverage": true,
  "collectCoverageFrom": ["**/src/**", "!**/src/__tests__/**"],
  "coverageDirectory": "coverage",
  "coverageProvider": "babel",
  "coverageReporters": ["lcov", "text", "text-summary", "json-summary"],
  "transform": {
    "\\.(ts)$": "ts-jest",
    "\\.[jt]sx?$": "babel-jest"
  },
  "testMatch": ["**/*.test.ts", "**/*.test.tsx"]
}
